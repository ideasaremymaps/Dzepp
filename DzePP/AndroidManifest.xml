<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dzepp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.Base" >
        <activity
            android:name="com.example.dzepp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.dzepp.RedcrossActivity"
            android:label="@string/title_activity_redcross" >
        </activity>
        <activity
            android:name="com.example.dzepp.CallActivity"
            android:label="@string/title_activity_call" >
        </activity>
        <activity
            android:name="com.example.dzepp.InfoActivity"
            android:label="@string/title_activity_info" >
        </activity>
        <activity
            android:name="com.example.dzepp.AmbulanceActivity"
            android:label="@string/title_activity_ambulance" >
        </activity>
    </application>

</manifest>
